{
  "name": "ts-bdd-check",
  "version": "1.1.1",
  "description": "Simple typescript wrapper for bdd and testcheck",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/justmiller/ts-lens"
  },
  "author": "Justin Miller<mogulslayer@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "tsc --outDir dist/",
    "test": "jest",
    "semantic-release": "semantic-release pre && semantic-release post",
    "lint": "tslint --project tsconfig.json --format stylish",
    "run-script": "npm run lint && npm run build && npm run test",
    "precommit": "./scripts/pre-commit.sh",
    "prepush": "npm run test",
    "commitmsg": "validate-commit-msg",
    "release": "standard-version && git push --follow-tags origin master"
  },
  "devDependencies": {
    "@types/jest": "21.1.5",
    "@types/lodash": "^4.14.62",
    "@types/node": "8.0.49",
    "child_process": "^1.0.2",
    "colors": "^1.1.2",
    "commitizen": "^2.9.6",
    "fs": "^0.0.1-security",
    "husky": "^0.14.3",
    "jest": "21.2.1",
    "path": "^0.12.7",
    "prettier": "^1.7.4",
    "standard-version": "^4.2.0",
    "ts-jest": "21.1.4",
    "ts-node": "^3.3.0",
    "tslint": "^5.7.0",
    "tslint-config-prettier": "^1.6.0",
    "typescript": "^2.2.2",
    "validate-commit-msg": "^2.14.0"
  },
  "prettier": {},
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(dist|src)/.*\\.spec\\.(js|ts)x?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  },
  "dependencies": {
    "testcheck": "^1.0.0-rc.2"
  }
}
